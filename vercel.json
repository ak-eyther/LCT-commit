{
  "buildCommand": null,
  "devCommand": null,
  "installCommand": null,
  "framework": null,
  "rewrites": [
    {
      "source": "/",
      "destination": "/src/app/index.html"
    },
    {
      "source": "/index.html",
      "destination": "/src/app/index.html"
    },
    {
      "source": "/login.html",
      "destination": "/src/app/login.html"
    },
    {
      "source": "/reports.html",
      "destination": "/src/app/reports.html"
    },
    {
      "source": "/settings.html",
      "destination": "/src/app/settings.html"
    },
    {
      "source": "/documentation.html",
      "destination": "/src/app/documentation.html"
    },
    {
      "source": "/team-structure-v2.html",
      "destination": "/src/app/team-structure-v2.html"
    },
    {
      "source": "/brain-dumps.html",
      "destination": "/src/app/brain-dumps.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    }
  ]
}
